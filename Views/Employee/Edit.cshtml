<h3>Edit EmployeeList</h3>
<div class="container">
    @{ Html.BeginForm();}
    <div class="row">
        <div class="col-lg-3">
            @Html.Label("Enter Employee Name")
        </div>
        <div class="col-lg-3">
            @Html.TextBox("empName",(string)ViewBag.empName)
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            @Html.Label("Enter Employee Department")
        </div>
        <div class="col-lg-3">
            @Html.TextBox("empDept", (string)ViewBag.empDept)
            @Html.Hidden("empId", (int)ViewBag.empId)
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
        </div>
        <div class="col-lg-3">
            <input type="submit" value="Update" class="btn btn-warning" />
        </div>
    </div>
    @{Html.EndForm();}
</div>
